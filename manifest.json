{
  "name": "Redmine Human time view",
  "version": "1.0",
  "description": "View human readable time",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [ "https://prj.adyax.com/*" ],
      "js": [ "/js/external/jquery.js", "/js/background.js" ],
	  "run_at": "document_end"
    }
  ],
  "icons": {
    "16":   "img/icon-16.png",
    "48":   "img/icon-48.png",
    "128":  "img/icon-128.png"
  }
}
